{
    "name": "symplify/autowire-array-parameter",
    "description": "Autowire array parameters for your Symfony applications",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "nette/utils": "^4.0.5",
        "symfony/dependency-injection": "^7.0"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpstan/extension-installer": "^1.2",
        "phpunit/phpunit": "^11.0",
        "symplify/easy-ci": "^11.1",
        "symplify/package-builder": "*",
        "symplify/phpstan-extensions": "^11.1",
        "symplify/symplify-kernel": "*",
        "tomasvotruba/class-leak": "^2.0.5",
        "tomasvotruba/unused-public": "^0.3.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../composer-json-manipulator"
        },
        {
            "type": "path",
            "url": "../easy-testing"
        },
        {
            "type": "path",
            "url": "../package-builder"
        },
        {
            "type": "path",
            "url": "../smart-file-system"
        },
        {
            "type": "path",
            "url": "../symplify-kernel"
        }
    ],
    "autoload": {
        "psr-4": {
            "Symplify\\AutowireArrayParameter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\AutowireArrayParameter\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "11.2-dev"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse --ansi  --error-format symplify",
        "release": "vendor/bin/monorepo-builder release patch --ansi"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "phpstan/extension-installer": true
        }
    }
}
