{
    "name": "symplify/symplify-kernel",
    "description": "Internal Kernel for Symplify packages",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symplify/autowire-array-parameter": "*",
        "symplify/package-builder": "*",
        "symplify/smart-file-system": "*",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpstan/extension-installer": "^1.2",
        "symplify/easy-ci": "^11.1",
        "symplify/phpstan-extensions": "^11.1",
        "tomasvotruba/class-leak": "^2.0.5",
        "tomasvotruba/unused-public": "^0.3.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../autowire-array-parameter"
        },
        {
            "type": "path",
            "url": "../easy-testing"
        },
        {
            "type": "path",
            "url": "../package-builder"
        },
        {
            "type": "path",
            "url": "../smart-file-system"
        }
    ],
    "autoload": {
        "psr-4": {
            "Symplify\\SymplifyKernel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SymplifyKernel\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "11.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse --ansi  --error-format symplify"
    },
    "config": {
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "phpstan/extension-installer": true
        }
    }
}
